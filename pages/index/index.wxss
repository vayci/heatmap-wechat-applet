/**index.wxss**/
.wrap {
    display: flex;
    flex-direction: row;
    overflow: scroll;
    width: 100%;
    -webkit-overflow-scrolling: touch;
}
.col {
    padding: 0px;
}
.item {
    background: #eee;
    height: 10px;
    width: 10px;
    margin: 2px;
    border-radius: 1px;
}
.item:hover {
    border: 1px solid black;
    height: 8px;
    width: 8px;
}
.weekItem {
    height: 10px;
    width: 20px;
    margin: 2px;
    font-size: 8px;
    text-align: center;
    font-family: 'Lato', "PingFang SC", "Microsoft YaHei", sans-serif;
    color: gray;
}
.green {
    background: greenyellow;
}